<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
</head>
<body>

    <nav class="navbar navbar-default navbar-fixed-top" id="headerNav">
        <div class="container-fluid">
   
            <div class="navbar-header">
                <a href="/Hjem/Index">
                    <img alt="Altmuligrart" src="~/Content/Bilder/12214375_10153378511903285_355566493_o.jpg" height="70px" width="70px">
                    </a>
            </div>
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <div class="btn-group" id="Kat">
                    @Html.ActionLink("Kategori", "Index", "Butikk", new { area = "" }, new { @class = "btn btn-default btn-lg" })
                    <button type="button" class="btn btn-info btn-lg dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <span class="caret"></span>
                        <span class="sr-only">Toggle Dropdown</span>
                    </button>
                    <ul class="dropdown-menu">
                        <li>@{Html.RenderAction("GenreMenu", "Butikk");}</li>
                     </ul>
                </div>

            <!--Høyre-->
                <ul class="nav navbar-nav navbar-right">
                    <li><span class="glyphicon glyphicon-shopping-cart aria-hidden=" true" id="vogna">@{Html.RenderAction("CartSummary", "Handlekurv");}</span></li>
                          
                    <li class="elementerInav">@{Html.RenderAction("loggetInn", "Konto");}</li>
                    <li class="elementerInav">@{Html.RenderAction("brukerPå", "Konto");}</li>
                    <li class="elementerInav">@Html.ActionLink("Admin", "Index", "ButikkSjef")</li>
                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>
   
    <div class="container body-content">
        @RenderBody()

       
    </div>
            <footer id="footerC">
                <p>&copy; @DateTime.Now.Year - Mats Løkken, Atle Aspelien og Lars Harald Jantli Kristiansen</p>
            </footer>
    <h1>ssssssss</h1>
        <script src="~/Scripts/jquery-1.10.2.min.js"></script>
        <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>